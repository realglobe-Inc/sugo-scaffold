/**
 * Get configurations
 * @function configs
 * @returns {Object}
 */
'use strict'

const pkg = require('../package.json')
const { nameColorizer } = require('apemancolor/lib/colorizers')

module.exports = function configs () {
  let color = nameColorizer('#3A8')(pkg.name)

  return {
    /** Dominant ui color */
    color
  }
}
