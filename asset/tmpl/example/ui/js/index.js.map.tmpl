    "/**\n * Editor for builder\n * @class ApBuilderEditor\n */\n\n'use strict'\n\nimport React, {PropTypes as types} from 'react'\nimport classnames from 'classnames'\nimport {ApEditor} from 'apeman-react-editor'\nimport {ApButtonGroup, ApButton, ApIconButton} from 'apeman-react-button'\n/** @lends ApBuilderEditor */\nconst ApBuilderEditor = React.createClass({\n  // --------------------\n  // Specs\n  // --------------------\n  propTypes: {\n    /** Handle for change */\n    onChange: types.func,\n    /** Handle for cancel */\n    onCancel: types.func,\n    /** Handle for submit */\n    onSubmit: types.func,\n    /** Label texts */\n    labels: types.object,\n    /** Editting source */\n    src: types.string,\n    /** Editting type */\n    mode: types.string,\n    /** Error message */\n    error: types.oneOfType([ types.string, types.object ]),\n    /** Show spinner */\n    spinning: types.bool,\n\n    /** Custom actions */\n    actions: types.array\n\n  },\n\n  getDefaultProps () {\n    return {\n      labels: {\n        cancel: 'Cancel',\n        submit: 'Submit'\n      },\n      actions: []\n    }\n  },\n\n  getInitialState () {\n    return {\n      actionBarOpen: false\n    }\n  },\n\n  render () {\n    const s = this\n    let { state, props } = s\n    let { labels, src } = props\n    let actions = s.getActions()\n    let hasActions = actions && actions.length > 0\n    return (\n      <div className={ classnames('ap-builder-editor', props.className) }>\n        <div className={ classnames('ap-builder-editor-main', {\n          'ap-builder-editor-main-with-action': hasActions\n        }) }>\n          <ApEditor value={ src }\n                    spinning={ props.spinning }\n                    mode={ props.mode }\n                    error={ props.error }\n                    onChange={ props.onChange }\n                    editorRef={ (editor) => s.editor = editor }\n          />\n          { hasActions ? s._renderActionBar(actions) : null }\n        </div>\n        <div className='ap-builder-editor-control'>\n          <ApButtonGroup>\n            <ApButton onTap={ s.handleCancel }> { labels.cancel }</ApButton>\n            <ApButton primary onTap={ s.handleSubmit }> { labels.submit }</ApButton>\n          </ApButtonGroup>\n        </div>\n      </div>\n    )\n  },\n  // --------------------\n  // Lifecycle\n  // --------------------\n\n  componentDidUpdate (prevProps, prevState) {\n    const s = this\n    let { editor } = s\n    if (editor) {\n      s._editorRefreshTimer = setTimeout(() => {\n        editor.refresh()\n      }, 300)\n    }\n  },\n\n  componentWillUnmount () {\n    const s = this\n    clearTimeout(s._editorRefreshTimer)\n  },\n\n  // --------------------\n  // Handle\n  // --------------------\n\n  handleCancel (e) {\n    const s = this\n    let { props } = s\n    let { mode, src } = props\n    if (props.onCancel) {\n      props.onCancel({ mode, value: src })\n    }\n  },\n\n  handleSubmit (e) {\n    const s = this\n    let { props } = s\n    let { mode, src } = props\n    if (props.onSubmit) {\n      props.onSubmit({ mode, value: src })\n    }\n  },\n\n  toggleActionBar () {\n    const s = this\n    let { state } = s\n    s.setState({ actionBarOpen: !state.actionBarOpen })\n  },\n\n  getActions () {\n    const s = this\n    let { props } = s\n    let { actions } = props\n    return actions && actions\n        .filter((action) => {\n          let { hidden } = action\n          if (typeof hidden === 'function') {\n            hidden = hidden()\n          }\n          return !hidden\n        })\n  },\n\n  // --------------------\n  // Private\n  // --------------------\n\n  _renderActionBar (actions) {\n    const s = this\n    let { state } = s\n    let { actionBarOpen } = state\n    return (\n      <div className={ classnames('ap-builder-editor-action-bar', {\n        'ap-builder-editor-action-bar-open': actionBarOpen\n      })}>\n        <ApIconButton simple\n                      className={ classnames(