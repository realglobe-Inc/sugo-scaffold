    "/**\n * Builder component\n * @class ApBuilder\n */\n\n'use strict'\n\nimport React, {PropTypes as types} from 'react'\nimport classnames from 'classnames'\nimport ApBuilderEditor from './ap_builder_editor'\nimport ApBuilderFrame from './ap_builder_frame'\nimport asleep from 'asleep'\nimport {ApSpinner} from 'apeman-react-spinner'\nimport {ApTab, ApTabItem, ApTabContent, ApTabGroup} from 'apeman-react-tab'\n\nlet noop = () => undefined\n\n/** @lends ApBuilder */\nconst ApBuilder = React.createClass({\n\n  // --------------------\n  // Specs\n  // --------------------\n\n  propTypes: {\n    /** Html contents */\n    html: types.string.isRequired,\n    /** Script text */\n    script: types.string,\n    /** Global variables */\n    globals: types.object,\n    /** Compile script before executing */\n    compile: types.func,\n    /** Handle html edit */\n    onHtmlEdit: types.func,\n    /** Handle script edit */\n    onScriptEdit: types.func,\n    /** Handle script error */\n    onScriptError: types.func,\n    /** Labels */\n    labels: types.object,\n    /** Errors */\n    errors: types.object,\n    /** Codemirror mode for html */\n    htmlMode: types.string,\n    /** Codemirror mode for script */\n    scriptMode: types.string,\n    /** Custom actions for html editor */\n    htmlActions: types.array,\n    /** Custom actions for script editor */\n    scriptActions: types.array,\n    /** Check if supported */\n    isSupported: types.func\n  },\n\n  mixins: [],\n\n  statics: {},\n\n  getInitialState () {\n    const s = this\n    return {\n      tab: null,\n      overlaying: false,\n      edittingHtml: '',\n      edittingScript: '',\n      vr: 0,\n      spinning: false,\n      htmlActions: [],\n      scriptActions: []\n    }\n  },\n\n  getDefaultProps () {\n    return {\n      html: '',\n      script: '',\n      onHtmlEdit: noop,\n      onScriptEdit: noop,\n      labels: {\n        htmlTab: 'HTML',\n        scriptTab: 'Script'\n      },\n      htmlMode: 'htmlmixed',\n 