    "/**\n * Style for ApResign.\n * @class ApResignStyle\n */\n\n'use strict'\n\nimport React, {PropTypes as types} from 'react'\nimport {ApStyle} from 'apeman-react-style'\n\n/** @lends ApResignStyle */\nconst ApResignStyle = React.createClass({\n  propTypes: {\n    style: types.object,\n    highlightColor: types.string\n  },\n  getDefaultProps () {\n    return {\n      style: {},\n      highlightColor: ApStyle.DEFAULT_HIGHLIGHT_COLOR,\n      backgroundColor: ApStyle.DEFAULT_BACKGROUND_COLOR\n    }\n  },\n  statics: {\n    styleData (config) {\n      let { highlightColor, backgroundColor } = config\n      return {\n        all: {\n          '.ap-resign': {},\n          '.ap-resign-lead-form-title': {\n            fontSize: '1.5em'\n          },\n          '.ap-resign-lead-form-message': {},\n          '.ap-resign-reason-form-title': {\n            fontSize: '1.5em'\n          },\n          '.ap-resign-reason-form-message': {},\n          '.ap-resign-lead-form-cancel-button .ap-prev-button-icon': {\n            display: 'none'\n          }\n        }\n\n      }\n    }\n  },\n  render () {\n    const s = this\n    let { props } = s\n\n    let { all, small, medium, large } = ApResignStyle.styleData(props)\n\n    return (\n      <ApStyle data={ Object.assign(all, props.style) }\n               smallMediaData={ small }\n               mediumMediaData={ medium }\n               largeMediaData={ large }\n      >{ props.children }</ApStyle>\n    )\n  }\n})\n\nexport default ApResignStyle\n",
    "/**\n * apeman react package for resign components\n * @module apeman-react-resign\n */\n\n'use strict'\n\nlet d = (module) => module.default || module\n\nmodule.exports = {\n  get ApResignLeadForm () { return d(require('./ap_resign_lead_form')) },\n  get ApResignReasonForm () { return d(require('./ap_resign_reason_form')) },\n  get ApResignStyle () { return d(require('./ap_resign_style')) },\n  get ApResign () { return d(require('./ap_resign')) }\n}\n",
    "/**\n * Style for ApSign.\n * @class ApSignStyle\n */\n\n'use strict'\n\nimport React, {PropTypes as types} from 'react'\nimport {ApStyle} from 'apeman-react-style'\n\n/** @lends ApSignStyle */\nconst ApSignStyle = React.createClass({\n  propTypes: {\n    style: types.object\n  },\n  getDefaultProps () {\n    return {\n      style: {}\n    }\n  },\n  statics: {\n    styleData (config) {\n      return {\n        all: {\n          '.ap-sign': {},\n          '.ap-signin-form': {\n            maxWidth: '396px'\n          },\n          '.ap-signin-form-button-field': {\n            margin: '8px 0',\n            padding: '0 8px'\n          },\n          '.ap-signin-form-menu-field': {\n            padding: '4px 8px'\n          },\n          '.ap-signin-form-menu-button-row': {\n            fontSize: '14px',\n            width: '100%',\n            margin: '8px 0'\n          },\n          '.ap-signin-form-action-button-row': {\n            padding: '0 4px'\n          },\n          '.ap-signin-form-action-button-row .ap-icon-button-text': {\n            fontSize: '11px'\n          },\n          '.ap-signup-form-button-field': {\n            margin: '8px 0',\n            padding: '0 8px'\n          }\n        }\n      }\n    }\n  },\n  render () {\n    const s = this\n    let { props } = s\n\n    let { all, small, medium, large } = ApSignStyle.styleData(props)\n\n    return (\n      <ApStyle data={ Object.assign(all, props.style) }\n               smallMediaData={ small }\n               mediumMediaData={ medium }\n               largeMediaData={ large }\n      >{ props.children }</ApStyle>\n    )\n  }\n})\n\nexport default ApSignStyle\n",
    "/**\n * Form for signin\n * @class ApSigninForm\n */\n\n'use strict'\n\nimport React, {PropTypes as types} from 'react'\nimport classnames from 'classnames'\n\nimport {ApForm} from 'apeman-react-form'\nimport {ApFormMixin} from 'apeman-react-mixin-form'\nimport {ApFieldSet, ApField, ApFieldLabel, ApFieldValue} from 'apeman-react-field'\nimport {ApButton, ApCellButton, ApCellButtonRow, ApIconButton, ApIconButtonRow} from 'apeman-react-button'\nimport {ApText} from 'apeman-react-text'\nimport {ApPassword} from 'apeman-react-password'\n\nconst noop = (e) => console.log(e)\n\n/** @lends ApSigninForm */\nconst ApSigninForm = React.createClass({\n\n  // --------------------\n  // Specs\n  // --------------------\n  propTypes: {\n    /** Menu names */\n    menu: types.arrayOf(types.string),\n    /** Button names */\n    buttons: types.arrayOf(types.string)\n  },\n\n  mixins: [\n    ApFormMixin\n  ],\n\n  statics: {},\n\n  getInitialState () {\n    const s = this\n    return {}\n  },\n\n  getDefaultProps () {\n    return {\n      formPrefix: 'signin',\n      formLabels: {\n        key: 'Username or Email',\n        password: 'Password',\n        submit: 'Sign In',\n        signup: 'Sign Up',\n        recover: 'Forget Password?',\n        twitter: 'Sign in with Twitter',\n        google: 'Sign in with Google',\n        facebook: 'Sign in with Facebook'\n      },\n      formActions: {\n        signup: noop,\n        recover: noop,\n        twitter: noop,\n        google: noop,\n        facebook: noop\n      },\n      formIcons: {\n        facebook: 'fa fa-facebook',\n        twitter: 'fa fa-twitter',\n        google: 'fa fa-google'\n      }\n    }\n  },\n\n  render () {\n    const s = this\n    let { props } = s\n    return (\n      <ApForm { ...props }\n        className={ classnames('ap-signin-form', props.className)}\n        style={ Object.assign({}, props.style) }\n      >\n        <ApFieldSet plain={ true }>\n          { s._renderKeyField() }\n          { s._renderPasswordField() }\n          { s._renderSubmitField() }\n        </ApFieldSet>\n        <ApFieldSet plain={ true }>\n          { s._renderMenuField(props.menu) }\n        </ApFieldSet>\n        {s._renderButtonField(props.buttons) }\n        <ApFieldSet plain={ true }>\n        </ApFieldSet>\n      </ApForm >\n    )\n  },\n\n  // --------------------\n  // Lifecycle\n  // --------------------\n\n  // ------------------\n  // Custom\n  // ------------------\n\n  // ------------------\n  // Private\n  // ------------------\n\n  _renderKeyField () {\n    const name = 'key'\n    const s = this\n    return (\n      <ApField>\n        <ApFieldLabel htmlFor={ s.getFormId(name) }>\n          { s.getFormLabel(name) }\n        </ApFieldLabel>\n        <ApFieldValue>\n          <ApText id={ s.getFormId(name) }\n                  name={ name }\n                  value={ s.getFormValue(name) }\n                  onChange={ s.handleFormChange }\n          />\n        </ApFieldValue>\n      </ApField>\n    )\n  },\n\n  _renderPasswordField () {\n    const name = 'password'\n    const s = this\n    return (\n      <ApField>\n        <ApFieldLabel htmlFor={ s.getFormId(name) }>\n          { s.getFormLabel(name) }\n        </ApFieldLabel>\n        <ApFieldValue>\n          <ApPassword id={ s.getFormId(name) }\n                      value={ s.getFormValue(name) }\n                      name={ name }\n                      onChange={ s.handleFormChange }\n          />\n        </ApFieldValue>\n      </ApField>\n    )\n  },\n\n  _renderSubmitField () {\n    const name = 'submit'\n    const s = this\n    return (\n      <ApField center={ true } className=\"ap-signin-form-button-field\">\n        <ApButton onTap={ s.handleFormSubmit }\n                  wide={ true }\n                  id={ s.getFormId(name) }\n                  primary={ true }\n        >{ s.getFormLabel(name) }</ApButton>\n      </ApField>\n    )\n  },\n\n  _renderMenuField (names) {\n    const s = this\n    if (!names) {\n      return null\n    }\n    return (\n      <ApField center={ true } className=\"ap-signin-form-action-field\">\n        <ApCellButtonRow className=\"ap-signin-form-action-button-row\">\n          {\n            names.map((name) => (\n              <ApCellButton key={ name }\n                            onTap={ s.asFormHandler(s.getFormAction(name)) }\n                            text={ s.getFormLabel(name) }/>\n            ))\n          }\n        </ApCellButtonRow>\n      </ApField>\n    )\n  },\n\n  _renderButtonField (names) {\n    const s = this\n    if (!names) {\n      return null\n    }\n    return (\n      <ApField className=\"ap-signin-form-action-field\">\n        <ApIconButtonRow className=\"ap-signin-form-action-button-row\">\n          {\n            names.map((name) => (\n              <ApIconButton onTap={ s.asFormHandler(s.getFormAction(name)) }\n                            key={ name }\n                            id={ s.getFormId(`action-${name}`) }\n                            icon={ s.getFormIcon(name) }\n                            text={ s.getFormLabel(name) }/>\n            ))\n          }\n        </ApIconButtonRow>\n      </ApField>\n    )\n  }\n})\n\nexport default ApSigninForm\n",
    "/**\n * Form for signin\n * @class ApSigninForm\n */\n\n'use strict'\n\nimport React, {PropTypes as types} from 'react'\nimport classnames from 'classnames'\nimport numcal from 'numcal'\n\nimport {ApForm} from 'apeman-react-form'\nimport {ApFormMixin} from 'apeman-react-mixin-form'\nimport {ApFieldSet, ApField, ApFieldLabel, ApFieldValue} from 'apeman-react-field'\nimport {ApButton, ApCellButton, ApCellButtonRow, ApIconButton, ApIconButtonRow} from 'apeman-react-button'\nimport {ApText} from 'apeman-react-text'\nimport {ApPassword} from 'apeman-react-password'\n\n/** @lends ApSigninForm */\nconst ApSigninForm = React.createClass({\n\n  // --------------------\n  // Specs\n  // --------------------\n\n  propTypes: {\n    email: types.bool,\n    password: types.bool\n  },\n\n  mixins: [\n    ApFormMixin\n  ],\n\n  statics: {},\n\n  getInitialState () {\n    const s = this\n    return {}\n  },\n\n  getDefaultProps () {\n    return {\n      formPrefix: 'signmod',\n      formLabels: {\n        email: 'Email',\n        password: 'Password',\n        submit: 'Update'\n      },\n      email: true,\n      password: true\n    }\n  },\n\n  render () {\n    const s = this\n    let { props } = s\n\n    return (\n      <ApForm { ...props }\n        className={ classnames('ap-signin-form', props.className)}\n        style={ Object.assign({}, props.style) }\n      >\n        <ApFieldSet plain={ true }>\n          { props.email ? s._renderEmailField() : null }\n          { props.password ? s._renderPasswordField() : null }\n        </ApFieldSet>\n        <ApFieldSet plain={ true }>\n          { s._renderSubmitField() }\n        </ApFieldSet>\n      </ApForm >\n    )\n  },\n\n  // --------------------\n  // Lifecycle\n  // --------------------\n\n  // ------------------\n  // Custom\n  // ------------------\n\n  // ------------------\n  // Private\n  // ------------------\n\n  _renderEmailField () {\n    const name = 'email'\n    const s = this\n\n    return (\n      <ApField className=\"ap-signmod-form-email-field\">\n        <ApFieldLabel htmlFor={ s.getFormId(name) }>\n          { s.getFormLabel(name) }\n        </ApFieldLabel>\n        <ApFieldValue>\n          <ApText id={ s.getFormId(name) }\n                  name={ name }\n                  value={ s.getFormValue(name) }\n                  onChange={ s.handleFormChange }\n          />\n        </ApFieldValue>\n      </ApField>\n    )\n  },\n\n  _renderPasswordField () {\n    const name = 'password'\n    const s = this\n\n    return (\n      <ApField>\n        <ApFieldLabel htmlFor={ s.getFormId(name) }>\n          { s.getFormLabel(name) }\n        </ApFieldLabel>\n        <ApFieldValue>\n          <ApPassword id={ s.getFormId(name) }\n                      value={ s.getFormValue(name) }\n                      name={ name }\n                      onChange={ s.handleFormChange }\n          />\n        </ApFieldValue>\n      </ApField>\n    )\n  },\n\n  _renderSubmitField () {\n    const name = 'submit'\n    const s = this\n    return (\n      <ApField center={ true } className=\"ap-signmod-form-button-field\">\n        <ApButton id={ s.getFormId(name) }\n                  onTap={ s.handleFormSubmit }\n                  wide={ true }\n                  primary={ true }\n        >\n          { s.getFormLabel(name) }\n        </ApButton>\n      </ApField>\n    )\n  }\n})\n\nexport default ApSigninForm\n",
    "/**\n * Form for signup\n * @class ApSignupForm\n */\n\n'use strict'\n\nimport React, {PropTypes as types} from 'react'\nimport classnames from 'classnames'\nimport numcal from 'numcal'\n\nimport {ApForm} from 'apeman-react-form'\nimport {ApFormMixin} from 'apeman-react-mixin-form'\nimport {ApFieldSet, ApField, ApFieldLabel, ApFieldValue} from 'apeman-react-field'\nimport {ApButton, ApCellButton, ApCellButtonRow, ApIconButton, ApIconButtonRow} from 'apeman-react-button'\nimport {ApText} from 'apeman-react-text'\nimport {ApPassword} from 'apeman-react-password'\nimport {ApCaptcha} from 'apeman-react-captcha'\n\nconst noop = (e) => console.log(e)\n\n/** @lends ApSigninForm */\nconst ApSigninForm = React.createClass({\n\n  // --------------------\n  // Specs\n  // --------------------\n\n  propTypes: {\n    captchaSpinning: types.bool\n  },\n\n  mixins: [\n    ApFormMixin\n  ],\n\n  statics: {},\n\n  getInitialState () {\n    const s = this\n    return {}\n  },\n\n  getDefaultProps () {\n    return {\n      formPrefix: 'signup',\n      formLabels: {\n        key: 'Username',\n        email: 'Email',\n        password: 'Password',\n        captcha: 'Captcha',\n        captchaRefresh: 'refresh',\n        submit: 'Sign Up'\n      },\n      formPlaceholders: {\n        captcha: '(Type numbers above)'\n      },\n      formImages: {\n        captcha: ''\n      },\n      formActions: {\n        captchaRefresh: noop\n      },\n      captchaSpinning: false\n    }\n  },\n\n  render () {\n    const s = this\n    let { props, state } = s\n\n    return (\n      <ApForm { ...props }\n        className={ classnames('ap-signin-form', props.className)}\n        style={ Object.assign({}, props.style) }\n      >\n        <ApFieldSet plain={ true }>\n          { s._renderKeyField() }\n          { s._renderEmailField() }\n          { s._renderPasswordField() }\n        </ApFieldSet>\n        <ApFieldSet plain={ true }>\n          { s._renderCaptchaField() }\n        </ApFieldSet>\n        <ApFieldSet plain={ true }>\n          { s._renderButtonField() }\n        </ApFieldSet>\n      </ApForm>\n    )\n  },\n\n  // --------------------\n  // Lifecycle\n  // --------------------\n\n  // ------------------\n  // Custom\n  // ------------------\n\n  // ------------------\n  // Private\n  // ------------------\n\n  _renderKeyField () {\n    const name = 'key'\n    const s = this\n    return (\n      <ApField>\n        <ApFieldLabel htmlFor={ s.getFormId(name) }>\n          { s.getFormLabel(name) }\n        </ApFieldLabel>\n        <ApFieldValue>\n          <ApText id={ s.getFormId(name) }\n                  name={ name }\n                  value={ s.getFormValue(name) }\n                  onChange={ s.handleFormChange }\n          />\n        </ApFieldValue>\n      </ApField>\n    )\n  },\n\n  _renderEmailField () {\n    const name = 'email'\n    const s = this\n    return (\n      <ApField>\n        <ApFieldLabel htmlFor={ s.getFormId(name) }>\n          { s.getFormLabel(name) }\n        </ApFieldLabel>\n        <ApFieldValue>\n          <ApText id={ s.getFormId(name) }\n                  name={ name }\n                  value={ s.getFormValue(name) }\n                  onChange={ s.handleFormChange }\n          />\n        </ApFieldValue>\n      </ApField>\n    )\n  },\n\n  _renderPasswordField () {\n    const name = 'password'\n    const s = this\n\n    return (\n      <ApField>\n        <ApFieldLabel htmlFor={ s.getFormId(name) }>\n          { s.getFormLabel(name) }\n        </ApFieldLabel>\n        <ApFieldValue>\n          <ApPassword id={ s.getFormId(name) }\n                      name={ name }\n                      value={ s.getFormValue(name) }\n                      onChange={ s.handleFormChange }\n          />\n        </ApFieldValue>\n      </ApField>\n    )\n  },\n\n  _renderCaptchaField () {\n    const name = 'captcha'\n    const refreshName = 'captchaRefresh'\n    const s = this\n    let { props } = s\n\n    return (\n      <ApField>\n        <ApFieldLabel htmlFor={ s.getFormId(name) }>\n          { s.getFormLabel(name) }\n        </ApFieldLabel>\n        <ApFieldValue>\n          <ApCaptcha src={ s.getFormImage(name) }\n                     spinning={ props.captchaSpinning }\n                     refreshText={ s.getFormLabel(refreshName) }\n                     onRefresh={ s.getFormAction(refreshName) }>\n          </ApCaptcha>\n          <ApText id={ s.getFormId(name) }\n                  name={ name }\n                  value={ s.getFormValue(name) }\n                  onChange={ s.handleFormChange }\n                  placeholder={ s.getFormPlaceholder(name) }\n          />\n        </ApFieldValue>\n      </ApField>\n    )\n  },\n\n  _renderButtonField () {\n    const name = 'submit'\n    const s = this\n    return (\n      <ApField center={ true } className=\"ap-signup-form-button-field\">\n        <ApButton id={ s.getFormId(name) }\n                  onTap={ s.handleFormSubmit }\n                  wide={ true }\n                  primary={ true }\n        >{ s.getFormLabel(name) }</ApButton>\n      </ApField>\n    )\n  }\n\n})\n\nexport default ApSigninForm\n",
    "/**\n * apeman react package for sign components\n * @module apeman-react-sign\n */\n\n'use strict'\n\nlet d = (module) => module.default || module\n\nmodule.exports = {\n  get ApSignStyle () { return d(require('./ap_sign_style')) },\n  get ApSigninForm () { return d(require('./ap_signin_form')) },\n  get ApSignmodForm () { return d(require('./ap_signmod_form')) },\n  get ApSignupForm () { return d(require('./ap_signup_form')) }\n}\n",
    "/**\n * apeman react package for switch components\n * @class ApSwitch\n */\n\n'use strict'\n\nimport React, {PropTypes as types} from 'react'\nimport classnames from 'classnames'\nimport {ApPureMixin} from 'apeman-react-mixin-pure'\nimport {ApTouchMixin} from 'apeman-react-mixin-touch'\nimport {ApUUIDMixin} from 'apeman-react-mixin-uuid'\n\n/** @lends ApSwitch */\nconst ApSwitch = React.createClass({\n\n  // --------------------\n  // Specs\n  // --------------------\n\n  propTypes: {\n    /** Switch on or not */\n    on: types.bool.isRequired,\n    /** Handle for tap event */\n    onTap: types.func.isRequired,\n    /** Title text for on state */\n    onTitle: types.string,\n    /** Title text for off state */\n    offTitle: types.string,\n    /** Width of component */\n    width: types.number\n  },\n\n  mixins: [\n    ApPureMixin,\n    ApTouchMixin,\n    ApUUIDMixin\n  ],\n\n  statics: {},\n\n  getInitialState () {\n    return {}\n  },\n\n  getDefaultProps () {\n    return {\n      on: false,\n      onTitle: '',\n      offTitle: ''\n    }\n  },\n\n  render () {\n    const s = this\n    let { state, props } = s\n    let { width } = props\n    let id = props.hasOwnProperty('id') ? props.id : s.uuid\n    let className = classnames('ap-switch', {\n      'ap-switch-on': props.on,\n      'ap-switch-off': !props.on\n    }, props.className)\n    return (\n      <div className={ className }\n           style={ Object.assign({width}, props.style) }\n           id={ id }\n      >\n        <div className=\"ap-switch-inner\">\n          { s._renderLabel(`${id}-radio-off`, 'ap-switch-on-label', props.onTitle) }\n          { s._renderRadio(`${id}-radio-off`, 'off', !props.on)}\n          <div className=\"ap-switch-handle\">\n          </div>\n          { s._renderLabel(`${id}-radio-on`, 'ap-switch-off-label', props.offTitle) }\n          { s._renderRadio(`${id}-radio-on`, 'on', !!props.on)}\n        </div>\n        { props.children }\n      </div>\n    )\n  },\n\n  // --------------------\n  // Lifecycle\n  // --------------------\n\n  // ------------------\n  // Custom\n  // ------------------\n\n  noop () {\n\n  },\n\n  // ------------------\n  // Private\n  // ------------------\n\n  _renderLabel (htmlFor, className, title) {\n    const s = this\n    return (\n      <label htmlFor={ htmlFor }\n             className={ classnames('ap-switch-label', className) }>\n        <span className=\"ap-switch-label-text\">{ title }</span>\n      </label>\n    )\n  },\n\n  _renderRadio (id, value, checked) {\n    const s = this\n    return (\n      <input type=\"radio\" id={ id }\n             value={ value }\n             checked={ checked }\n             onChange={ s.noop }\n             className=\"ap-switch-radio\"/>\n    )\n  }\n})\n\nexport default ApSwitch\n",
