    "/**\n * Example instaruction component\n * @class SgExampleInstruction\n */\n\n'use strict'\n\nimport React, {PropTypes as types} from 'react'\nimport classnames from 'classnames'\nimport {ApSection, ApSectionHeader, ApSectionBody} from 'apeman-react-section'\nimport {ApMarkdown} from 'apeman-react-markdown'\nimport {injectVariables} from './helpers/markdown_helper'\n\n/** @lends SgExampleInstruction */\nconst SgExampleInstruction = React.createClass({\n\n  // --------------------\n  // Specs\n  // --------------------\n\n  propTypes: {\n    pkg: types.object,\n    /** Texts for display */\n    messages: types.object,\n    /** Source markdown texts */\n    src: types.array,\n    links: types.object,\n    vars: types.object\n  },\n\n  getDefaultProps () {\n    return {\n      messages: {\n        header: 'Instructions'\n      },\n      vars: {},\n      links: {}\n    }\n  },\n\n  render () {\n    const s = this\n    let { props } = s\n    let { src, messages, links, vars } = props\n    return (\n      <ApSection className='sg-example-card-section'>\n        <ApSectionHeader lined>{ messages.header }</ApSectionHeader>\n        <ApSectionBody>\n          <div className='sg-example-instruction'>\n            {\n              [].concat(src).map((src, i) => (\n                <ApMarkdown src={ src }\n                            beforeCompile={ (src) => injectVariables(src, vars) }\n                            key={ i }\n                            links={ links }\n                />\n              ))\n            }\n          </div>\n        </ApSectionBody>\n      </ApSection>\n    )\n  }\n\n})\n\nexport default SgExampleInstruction\n",
    "/**\n * @class SgExampleLinks\n */\n'use strict'\n\nimport React, {PropTypes as types} from 'react'\nimport classnames from 'classnames'\nimport {ApLinks} from 'apeman-react-links'\nimport {ApSection, ApSectionHeader, ApSectionBody} from 'apeman-react-section'\n\n/** @lends SgExampleLinks */\nconst SgExampleLinks = React.createClass({\n  // --------------------\n  // Specs\n  // --------------------\n\n  propTypes: {\n    /** Texts for display */\n    messages: types.object,\n    /** Link data */\n    links: types.object\n  },\n\n  getDefaultProps () {\n    return {\n      messages: {\n        header: 'Links'\n      },\n      links: {}\n    }\n  },\n\n  render () {\n    const s = this\n    let { props } = s\n    let { messages, links } = props\n    return (\n      <ApSection className='sg-example-card-section'>\n        <ApSectionHeader lined>{ messages.header }</ApSectionHeader>\n        <ApSectionBody>\n          <ApLinks className='sg-example-links'\n                   links={ links }/>\n        </ApSectionBody>\n      </ApSection>\n    )\n  }\n})\n\nexport default SgExampleLinks\n",
    "/**\n * Playground for example\n * @class SgExamplePlayground\n */\n'use strict'\n\nimport React, {PropTypes as types} from 'react'\nimport classnames from 'classnames'\nimport {ApButtonGroup, ApButton, ApIconButton} from 'apeman-react-button'\nimport {ApBuilder} from 'apeman-react-builder'\nimport {ApConsole} from 'apeman-react-console'\nimport {ApLayoutMixin} from 'apeman-react-mixin-layout'\nimport {ApSection, ApSectionHeader, ApSectionBody} from 'apeman-react-section'\nimport {toggleBodyScroll} from './helpers/body_helper'\n\n/** @lends SgExamplePlayground */\nconst SgExamplePlayground = React.createClass({\n  // --------------------\n  // Specs\n  // --------------------\n\n  propTypes: {\n    /** Html contents */\n    html: types.string.isRequired,\n    /** Script text */\n    script: types.string,\n    /** Global variables */\n    globals: types.object,\n    /** Compile script before executing */\n    compile: types.func,\n    /** Time stamp */\n    timestamp: types.object,\n    /** Change handler */\n    onChange: types.func,\n    /** Message resources */\n    messages: types.object,\n    /** Pipe console output */\n    pipeConsole: types.bool,\n    /** Hidden to display */\n    closed: types.bool,\n    /** Handle toggle */\n    onToggle: types.func,\n    /** Default html string */\n    defaultHtml: types.string,\n    /** Default script string */\n    defaultScript: types.string\n  },\n\n  mixins: [\n    ApLayoutMixin\n  ],\n\n  getDefaultProps () {\n    return {\n      onChange: () => null,\n      messages: {\n        compileStart: 'Start compiling....',\n        compileDone: '...compile done!',\n        title: 'PlayGround',\n        open: 'Open Playground',\n        header: 'Playground',\n        restoreDefault: 'Restore default'\n      },\n      closed: false\n    }\n  },\n\n  getInitialState () {\n    const s = this\n    let { props, state } = s\n    return {\n      heightRate: 0.7,\n      scriptError: null\n    }\n  },\n\n  render () {\n    const s = this\n    let { props, state, layouts } = s\n    let { html, script, messages, closed } = props\n\n    let className = classnames('sg-example-playground', {\n      'sg-example-playground-has-script-error': !!state.scriptError\n    }, props.className)\n\n    return (\n      <ApSection className={ className }\n                 style={ Object.assign({}, props.style) }\n      >\n        <ApSectionHeader lined>\n          <i className='sg-example-section-icon fa fa-flag-checkered'/>\n          { messages.header }\n        </ApSectionHeader>\n        <ApSectionBody>\n          <div className='sg-example-action'>\n            <ApButtonGroup centered>\n              <ApButton onTap={ props.onToggle }\n                        primary\n              >{ messages.open }</ApButton>\n            </ApButtonGroup>\n          </div>\n        </ApSectionBody>\n        <div className={ classnames('sg-example-dialog', {\n          'sg-example-dialog-closed': closed\n        }) }\n             style={ layouts.playground }\n        >\n          <div className='sg-example-navbar'>\n          <span className='sg-example-navbar-side'>\n          </span>\n          <span className='sg-example-navbar-title'>\n            { messages.title }\n          </span>\n          <span className='sg-example-navbar-side'>\n          <ApIconButton className='sg-example-navbar-button fa fa-close'\n                        simple\n                        onTap={ props.onToggle }\n          />\n          </span>\n          </div>\n          { closed ? null : s._renderBuilder(html, script) }\n          <div className='sg-example-playground-console'>\n            <ApConsole consoleRef={ s.registerConsole }\n                       style={ layouts.console }\n                       height={ layouts.console.height }\n                       pipe={ props.pipeConsole }\n            />\n          </div>\n        </div>\n      </ApSection>\n    )\n  },\n\n  // --------------------\n  // Lifecycle\n  // --------------------\n\n  componentDidMount () {\n    const s = this\n    let { props } = s\n\n    s.console = window.console\n\n    toggleBodyScroll(props.closed)\n  },\n\n  componentWillReceiveProps (nextProps) {\n    const s = this\n    let { props } = s\n  },\n\n  componentDidUpdate () {\n    const s = this\n    let { props } = s\n\n    toggleBodyScroll(props.closed)\n  },\n\n  // --------------------\n  // For ApLayoutMixin\n  // --------------------\n\n  getInitialLayouts () {\n    const s = this\n    let height = 512\n    return {\n      playground: { height },\n      builder: s.builderStyle(height),\n      console: s.consoleStyle(height)\n    }\n  },\n\n  calcLayouts () {\n    const s = this\n    let { innerHeight } = window\n    let navbarHeight = 36\n    let height = innerHeight - navbarHeight\n    return {\n      playground: { height },\n      builder: s.builderStyle(height),\n      console: s.consoleStyle(height)\n    }\n  },\n\n  // --------------------\n  // Handle\n  // --------------------\n\n  compileScript (script) {\n    const s = this\n    let { props } = s\n    let { messages, compile } = props\n    if (!script) {\n      return ''\n    }\n    s.setState({ scriptError: null })\n    return new Promise((resolve) =>\n      // Wait for console\n      setTimeout(() => resolve(), 50)\n    ).then(() => {\n      if (!compile) {\n        return script\n      }\n      let { console } = s\n      let startAt = new Date()\n      return Promise.resolve(script)\n        .then((script) => {\n          console.debug(messages.compileStart)\n          return script\n        })\n        .then((script) => compile(script))\n        .then((script) => {\n          let took = new Date() - startAt\n          console.debug(`${messages.compileDone} (${took}ms)`)\n          return script\n        })\n    })\n  },\n\n  getGlobals () {\n    const s = this\n    let { props, console } = s\n\n    return Object.assign({}, props.globals, console ? { console } : {})\n  },\n\n  getErrors () {\n    const s = this\n    let { state } = s\n    let { scriptError } = state\n    if (scriptError) {\n      return { script: scriptError }\n    }\n  },\n\n  registerConsole (console) {\n    const s = this\n    s.console = console\n  },\n\n  handleHtmlEdit (e) {\n    const s = this\n    s.handleChange({\n      html: e.value\n    })\n  },\n\n  handleScriptEdit (e) {\n    const s = this\n    s.handleChange({\n      script: e.value\n    })\n  },\n\n  handleScriptError (scriptError) {\n    const s = this\n    let { console } = s\n    console.error(scriptError)\n    s.setState({ scriptError })\n  },\n\n  handleChange (changed) {\n    const s = this\n    let { props } = s\n    let { html, script, globals, onChange } = props\n    let values = Object.assign({\n      html, script, globals\n    }, changed)\n    onChange({ values })\n  },\n\n  builderStyle (height) {\n    const s = this\n    let { heightRate } = s.state\n    return {\n      height: height * heightRate\n    }\n  },\n\n  consoleStyle (height) {\n    const s = this\n    let { heightRate } = s.state\n    return {\n      height: height * (1 - heightRate)\n    }\n  },\n\n  console: undefined,\n\n  _renderBuilder (html, script) {\n    const s = this\n    let { layouts, props } = s\n    let { messages, defaultHtml, defaultScript } = props\n    let htmlActions = [\n      {\n        hidden: () => s.builder && s.builder.state.edittingHtml === defaultHtml,\n        text: messages.restoreDefault,\n        handle () { s.builder.setState({ edittingHtml: defaultHtml }) }\n      }\n    ]\n    let scriptActions = [\n      {\n        hidden: () => s.builder && s.builder.state.edittingScript === defaultScript,\n        text: messages.restoreDefault,\n        handle () { s.builder.setState({ edittingScript: defaultScript }) }\n      }\n    ]\n\n    return (\n      <div className='sg-example-playground-builder'>\n        <ApBuilder ref={ (builder) => s.builder = builder}\n                   html={ html }\n                   script={ script }\n                   compile={ s.compileScript }\n                   globals={ s.getGlobals() }\n                   onHtmlEdit={ s.handleHtmlEdit }\n                   onScriptError={ s.handleScriptError }\n                   onScriptEdit={ s.handleScriptEdit }\n                   style={ layouts.builder }\n                   errors={ s.getErrors() }\n                   scriptMode='jsx'\n                   htmlActions={ htmlActions }\n                   scriptActions={ scriptActions }\n        />\n      </div>\n    )\n  }\n})\n\nexport default SgExamplePlayground\n",
    "/**\n * Status for example\n * @class SgExampleStatus\n */\n'use strict'\n\nimport React, {PropTypes as types} from 'react'\nimport classnames from 'classnames'\nimport {\n  ApInfo,\n  ApInfoItem,\n  ApInfoHeader,\n  ApInfoCollapsibleItem,\n  ApInfoDisclosableItem,\n  ApInfoBody\n} from 'apeman-react-info'\nimport {ApSection, ApSectionHeader, ApSectionBody} from 'apeman-react-section'\nimport {ApSpinner} from 'apeman-react-spinner'\nimport {ApButtonGroup, ApButton, ApIconButton} from 'apeman-react-button'\n\n/** @lends SgExampleStatus */\nconst SgExampleStatus = React.createClass({\n  // --------------------\n  // Specs\n  // --------------------\n\n  propTypes: {\n    /** Data of spots */\n    spots: types.array,\n    /** Data of terminals */\n    terminals: types.array,\n    /** Texts for display */\n    messages: types.object,\n    /** Show spinning */\n    spinning: types.bool,\n\n    onRefresh: types.func\n  },\n\n  getDefaultProps () {\n    return {\n      spots: null,\n      terminals: null,\n      cloud: null,\n      spinning: false,\n      messages: {\n        header: 'Cloud Server Status',\n        cloud: 'Cloud',\n        host: 'Host',\n        spots: 'Spot Connections',\n        terminals: 'Terminal Connections',\n        connectionUnit: 'connections',\n        interfaceUnit: 'interfaces',\n        spotUnit: 'spots',\n        refresh: 'Refresh Status',\n        back: '<< back'\n      }\n    }\n  },\n\n  getInitialState () {\n    return {}\n  },\n\n  render () {\n    const s = this\n    let { props, state } = s\n    let { spots, terminals, cloud, messages } = props\n    spots = spots || []\n    terminals = terminals || []\n    return (\n      <ApSection>\n        <ApSectionHeader lined>\n          <i className='sg-example-section-icon fa fa-cloud'/>\n          { messages.header }\n        </ApSectionHeader>\n        <ApSectionBody>\n          <ApSpinner className='sg-section-spinner'\n                     size='xx-large'\n                     enabled={ props.spinning }\n          />\n          <div className='sg-example-status-body'>\n            <ApInfo className='sg-example-status-info'>\n              <ApInfoBody>\n                <ApInfoItem label={ messages.host }\n                            value={ cloud && cloud.host }/>\n                { s._renderSpotsItem(spots || []) }\n                { s._renderTerminalItem(terminals || []) }\n              </ApInfoBody>\n            </ApInfo>\n          </div>\n          <div className='sg-example-action'>\n            <ApButtonGroup centered>\n              <ApButton onTap={ props.onRefresh }\n              >{ messages.refresh }</ApButton>\n            </ApButtonGroup>\n          </div>\n        </ApSectionBody>\n      </ApSection>\n    )\n  },\n\n  // --------------------\n  // Custom\n  // --------------------\n\n  registerRootCollapsible (collapsible) {\n    const s = this\n    s._nestedCollapsibles = s._nestedCollapsibles || []\n    s._nestedCollapsibles.push(collapsible)\n  },\n\n  layoutRootCollapsibles (e) {\n    const s = this\n    for (let collapsible of s._nestedCollapsibles || []) {\n      collapsible.layout()\n    }\n  },\n\n  _renderSpotsItem (spots) {\n    const s = this\n    let { messages } = s.props\n    if (spots.length === 0) {\n      return (\n        <ApInfoItem label={ messages.spots }\n                    value='0'\n                    unit={ messages.connectionUnit }\n        />\n      )\n    }\n    return (\n      <ApInfoCollapsibleItem label={ messages.spots }\n                             value={ String(spots.length) }\n                             unit={ messages.connectionUnit }\n                             ref={ s.registerRootCollapsible }\n      >\n        <ApInfo>\n          <ApInfoBody>\n            { (spots || []).map((spot) => s._renderSpotsItemItem(spot)) }\n          </ApInfoBody>\n        </ApInfo>\n      </ApInfoCollapsibleItem>\n    )\n  },\n\n  _renderSpotsItemItem  (spot) {\n    const s = this\n    let { messages } = s.props\n    let { key, $specs } = spot\n    return (\n      <ApInfoCollapsibleItem key={ key }\n                             label={ key }\n                             value={ String(Object.keys($specs || {}).length) }\n                             unit={ messages.interfaceUnit }\n                             onToggle={ () => s.layoutRootCollapsibles() }\n      >\n        <ApInfo>\n          <ApInfoBody>\n            { Object.keys($specs || {}).map((name) => (\n              <ApInfoItem label={ name }\n                          key={ name }\n              />\n            ))}\n          </ApInfoBody>\n        </ApInfo>\n      </ApInfoCollapsibleItem>\n    )\n  },\n\n  _renderTerminalItem (terminals) {\n    const s = this\n    let { messages } = s.props\n    if (terminals.length === 0) {\n      return (\n        <ApInfoItem label={ messages.terminals }\n                    value='0'\n                    unit={ messages.connectionUnit }\n        />\n      )\n    }\n    return (\n      <ApInfoCollapsibleItem label={ messages.terminals }\n                             value={ String(terminals.length) }\n                             unit={ messages.connectionUnit }\n                             ref={ s.registerRootCollapsible }\n      >\n        <ApInfo>\n          <ApInfoBody>\n            {\n              (terminals || []).map((terminal) => s._renderTerminalItemItem(terminal))\n            }\n          </ApInfoBody>\n        </ApInfo>\n      </ApInfoCollapsibleItem>\n    )\n  },\n\n  _renderTerminalItemItem (terminal) {\n    const s = this\n    let { messages } = s.props\n    let { key, spots } = terminal\n    let hasSpots = (spots || []).length > 0\n    if (!hasSpots) {\n      return (\n        <ApInfoItem key={ key }\n                    label={ key }\n                    value='0'\n                    unit={ messages.spotUnit }\n        />\n      )\n    }\n    return (\n      <ApInfoCollapsibleItem key={ key }\n                             label={ key }\n                             value={ String((spots).length) }\n                             unit={ messages.spotUnit }\n                             onToggle={ () => s.layoutRootCollapsibles() }\n      >\n        <ApInfo>\n          <ApInfoBody>\n            { spots.map((name) => (\n              <ApInfoItem key={ `${key}-spot-${name}` }\n                          label='&nbsp;'\n                          value={ name }/>\n            )) }\n          </ApInfoBody>\n        </ApInfo>\n      </ApInfoCollapsibleItem>\n    )\n  }\n})\n\nexport default SgExampleStatus\n",
    "/**\n * Style for SgExample.\n * @class SgExampleStyle\n */\n\n'use strict'\n\nimport React, {PropTypes as types} from 'react'\nimport {SgThemeStyle} from 'sg-react-components'\nimport {ApStyle} from 'apeman-react-style'\nimport {ApLinksStyle} from 'apeman-react-links'\nimport {ApBuilderStyle} from 'apeman-react-builder'\nimport {ApConsoleStyle} from 'apeman-react-console'\nimport {ApMarkdownStyle} from 'apeman-react-markdown'\n\nlet { stringifyStyle, styleContent } = ApStyle\n\n/** @lends SgExampleStyle */\nconst SgExampleStyle = React.createClass({\n  propTypes: {\n    style: types.object,\n    /** Dominant color */\n    dominant: types.string\n  },\n  getDefaultProps () {\n    return {\n      style: {}\n    }\n  },\n  statics: {\n    stringifyStyle,\n    styleContent\n  },\n  render () {\n    const s = this\n    let { props } = s\n    let { dominant } = props\n    let navbarHeight = 36\n\n    let animationDuration = 700\n\n    let style = {\n      '.sg-example': {\n        fontFamily: 'ヒラギノ角ゴ ProN W3, Hiragino Kaku Gothic ProN, メイリオ, Meiryo, sans-serif'\n      },\n      '.sg-example-header.ap-header': {\n        borderBottom: '1px solid #EEE'\n      },\n      '.sg-example-header.ap-header .ap-header-logo': {\n        padding: '0 2px'\n      },\n      '.sg-example-playground .ap-builder': {\n        height: 320\n      },\n      '.sg-example-body-hidden': {\n        display: 'none !important'\n      },\n      '.sg-example-dialog': {\n        position: 'fixed',\n        left: 0,\n        top: 0,\n        right: 0,\n        height: '100%',\n        zIndex: 44,\n        backgroundColor: 'white',\n        transition: `top ${animationDuration}ms`\n      },\n      '.sg-example-dialog-closed': {\n        top: '100% !important'\n      },\n      '.sg-example-no-scroll': {\n        overflow: 'hidden'\n      },\n      '.sg-example-navbar': {\n        display: 'flex',\n        alignItems: 'center',\n        justifyContent: 'space-between',\n        height: `${navbarHeight}px`,\n        lineHeight: `${navbarHeight}px`\n      },\n      '.sg-example-navbar-title': {\n        color: '#888'\n      },\n      '.sg-example-navbar-side': {\n        display: 'inline-block',\n        width: '48px'\n      },\n      '.sg-example-navbar-button': {},\n      '.sg-example-action': {\n        textAlign: 'center',\n        padding: '16px 0 8px',\n        margin: '0 auto'\n      },\n      '.sg-example-links a:link': {\n        color: dominant,\n        textDecoration: 'none'\n      },\n      '.sg-example-links a:link:hover': {\n        textDecoration: 'underline'\n      },\n      '.sg-example-tooltip': {\n        position: 'fixed',\n        boxSizing: 'border-box',\n        transition: `left ${animationDuration}ms, top ${animationDuration}ms, right ${animationDuration}ms, bottom ${animationDuration}ms`,\n        zIndex: 24,\n        background: ApStyle.COVER_BACKGROUND,\n        top: 0,\n        left: 0,\n        right: 0,\n        bottom: 0,\n        overflow: 'hidden',\n        border: '1px solid #EEE'\n      },\n      '.sg-example-tooltip pre': {\n        background: '#FAFAFA',\n        border: '1px solid #F0F0F0',\n        padding: '8px 4px',\n        boxSizing: 'border-box',\n        color: '#555'\n      },\n      '.sg-example-tooltip-hidden': {\n        right: '50%',\n        left: '50%',\n        top: '50%',\n        bottom: '50%',\n        pointerEvents: 'none'\n      },\n      '.sg-example-tooltip-content': {\n        margin: '8px 16px',\n        padding: '8px',\n        background: '#FFF',\n        border: '1px solid #F0F0F0',\n        borderRadius: '4px'\n      },\n      '.sg-example-snippet': {\n        padding: '8px 4px',\n        background: '#F0F0F0',\n        fontSize: 'small',\n        margin: '4px 0',\n        borderRadius: '8px',\n        border: '1px solid #E0E0E0'\n      },\n      '.sg-example-about': {\n        padding: '0 2px'\n      },\n      '.sg-example-about a': {\n        color: dominant\n      },\n      '.sg-example-instruction pre': {\n        background: '#FAFAFA',\n        border: '1px solid #F0F0F0',\n        padding: '8px 4px',\n        boxSizing: 'border-box',\n        color: '#555',\n        overflow: 'auto'\n      },\n      '.sg-example .ap-section-header-lined': {\n        borderBottomColor: dominant\n      },\n      '.sg-example .sg-example-section-icon': {\n        background: dominant,\n        display: 'inline-flex',\n        marginRight: '2px',\n        color: 'white',\n        borderRadius: '50%',\n        justifyContent: 'center',\n        alignItems: 'center',\n        fontSize: '10px',\n        width: '16px',\n        height: '16px',\n        position: 'relative',\n        top: '-2px'\n      },\n      '.sg-example .ap-section': {\n        position: 'relative'\n      },\n      '.sg-example .ap-section-body': {\n        position: 'relative'\n      },\n      '.sg-example .sg-section-spinner': {\n        position: 'absolute',\n        left: 0,\n        top: 0,\n        right: 0,\n        bottom: 0,\n        zIndex: 3,\n        background: ApStyle.COVER_BACKGROUND,\n        color: ApStyle.COVER_COLOR\n      },\n      '.sg-example .ap-info-collapsible-wrap .ap-info-collapsible-wrap': {\n        transition: 'none'\n      },\n      '.sg-example .sg-example-card-section': {\n        border: '1px solid #E0E0E0',\n        padding: '4px 8px',\n        boxShadow: '1px 1px 2px rgba(48,48,48,0.1)',\n        borderRadius: '4px',\n        background: '#FFF'\n      },\n      '.sg-example .sg-example-card-section .ap-section-header-lined': {\n        padding: '4px',\n        color: 'inherit',\n        fontSize: '16px'\n      },\n      '.sg-example .sg-example-card-section .ap-section-body': {\n        padding: '2px 4px'\n      },\n      '.sg-example .ap-selectable-article-header .ap-label': {\n        paddingRight: '4px',\n        color: '#888'\n      },\n      '.sg-example .ap-big-button': {\n        textAlign: 'center'\n      },\n      '.sg-example .ap-info-disclosable-back': {\n        justifyContent: 'center'\n      }\n    }\n    return (\n      <ApStyle >\n        { stringifyStyle(Object.assign(style, props.style)) }\n        { styleContent(<SgThemeStyle dominant={ dominant }/>) }\n        { styleContent(<ApBuilderStyle highlightColor={ dominant }/>) }\n        { styleContent(<ApConsoleStyle highlightColor={ dominant }/>) }\n        { styleContent(<ApLinksStyle highlightColor={ dominant }/>) }\n        { styleContent(<ApMarkdownStyle highlightColor={ dominant }/>) }\n        { props.children }\n      </ApStyle>\n    )\n  }\n})\n\nexport default SgExampleStyle\n",
