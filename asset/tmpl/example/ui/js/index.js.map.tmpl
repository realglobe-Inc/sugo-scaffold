    "/**\n * Style for ApBuilder.\n * @constructor ApBuilderStyle\n */\n\n'use strict'\n\nimport React, {PropTypes as types} from 'react'\nimport {ApStyle} from 'apeman-react-style'\nimport {ApEditorStyle} from 'apeman-react-editor'\nimport {alpha} from 'apemancolor'\n\n/** @lends ApBuilderStyle */\nconst ApBuilderStyle = React.createClass({\n  propTypes: {\n    style: types.object,\n    highlightColor: types.string\n  },\n  getDefaultProps () {\n    return {\n      style: {},\n      highlightColor: ApStyle.DEFAULT_HIGHLIGHT_COLOR,\n      backgroundColor: ApStyle.DEFAULT_BACKGROUND_COLOR\n    }\n  },\n  statics: {\n    styleData (config) {\n      let { highlightColor, backgroundColor } = config\n      let tabHeight = 24\n      let tabMargin = 8\n      let tabCorner = 4\n      let editorControlHeight = 64\n      let animationDuration = 400\n\n      let darkBorderColor = '#AAA'\n\n      let editorPadding = 4\n      let actionBarHeight = 32\n      let toggleButtonHeight = 24\n      return {\n        all: {\n          '.ap-builder': {\n            display: 'block',\n            position: 'relative',\n            border: '1px solid #F0F0F0',\n            background: backgroundColor,\n            overflow: 'hidden'\n          },\n          '.ap-builder .ap-builder-frame': {\n            border: '1px solid #F0F0F0',\n            width: '100%',\n            height: '100%',\n            boxSizing: 'border-box'\n          },\n          '.ap-builder .ap-builder-overlay': {\n            position: 'absolute',\n            transform: 'translateZ(0)',\n            left: 0,\n            bottom: 0,\n            right: 0,\n            height: 0,\n            boxSizing: 'border-box',\n            background: 'rgba(250, 250, 250, 0.99)',\n            padding: 0,\n            transition: `height ${animationDuration}ms, padding-top ${animationDuration}ms`,\n            border: 'none',\n            boxShadow: '1px 1px 2px rgba(0,0,0,0.1)',\n            top: 'auto'\n          },\n         