    "/**\n * Toast group.\n * @class ApToastGroup\n */\n\n'use strict'\n\nimport React, {PropTypes as types} from 'react'\nimport ApToast from './ap_toast'\nimport {ApPureMixin} from 'apeman-react-mixin-pure'\nimport ApInfoToast from './ap_info_toast'\nimport ApWarnToast from './ap_warn_toast'\nimport ApErrorToast from './ap_error_toast'\nimport Toaster from './toasting/toaster'\nimport classnames from 'classnames'\n\n/** @lends ApToastGroup */\nconst ApToastGroup = React.createClass({\n\n  // --------------------\n  // Specs\n  // --------------------\n\n  propTypes: {\n    duration: types.number,\n    toaster: types.instanceOf(Toaster)\n  },\n\n  mixins: [\n    ApPureMixin\n  ],\n\n  statics: {\n    Toaster: Toaster\n  },\n\n  getInitialState () {\n    let state = { _date: null }\n    Toaster.levels.forEach(level => {\n      state[ level ] = null;\n    })\n    return state;\n  },\n\n  getDefaultProps () {\n    return {\n      toaster: null\n    }\n  },\n\n  render () {\n    const s = this\n    let { state, props } = s\n    let { duration } = props\n\n    return (\n      <div className={ classnames('ap-toast-group', {\n            }, props.className) }>\n        <ApToast message={ state.default } duration={ duration } name=\"default\" onDismiss={ s.handleDismiss }/>\n        <ApInfoToast message={ state.info } duration={ duration } name=\"info\" onDismiss={ s.handleDismiss }/>\n        <ApWarnToast message={ state.warn } duration={ duration } name=\"warn\" onDismiss={ s.handleDismiss }/>\n        <ApErrorToast message={ state.error } duration={ duration } name=\"error\" onDismiss={ s.handleDismiss }/>\n        { props.children }\n      </div>\n    )\n  },\n\n  // --------------------\n  // Lifecycle\n  // --------------------\n\n  componentDidMount () {\n    const s = this\n    let { props } = s\n    if (props.toaster) {\n      s.bindToaster(props.toaster)\n    }\n  },\n\n  componentWillReceiveProps (nextProps) {\n    const s = this\n    let { props } = s\n    if (