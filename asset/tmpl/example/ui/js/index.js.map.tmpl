    "/**\n * Frame for builder\n * @class ApBuilderFrame\n */\n\n'use strict'\n\nimport React, {PropTypes as types} from 'react'\nimport classnames from 'classnames'\nimport {ApFrame} from 'apeman-react-frame'\nimport {ApSpinner} from 'apeman-react-spinner'\nimport {ApUUIDMixin} from 'apeman-react-mixin-uuid'\nimport equal from 'deep-equal'\nimport defineDebug from 'debug'\nimport asleep from 'asleep'\n\nconst debug = defineDebug('ap:builder:frame')\n\n/** @lends ApBuilderFrame */\nconst ApBuilderFrame = React.createClass({\n  // --------------------\n  // Specs\n  // --------------------\n  propTypes: {\n    /** Source string */\n    src: types.string,\n    /** Handler for load event */\n    onLoad: types.func,\n    /** Handler for script error */\n    onScriptError: types.func,\n    /** Global variables inside iframe */\n    globals: types.object,\n    /** Script to execute when loaded */\n    script: types.string,\n    /** Compile script before executing */\n    compile: types.func,\n    /** Version number */\n    vr: types.number\n  },\n\n  getDefaultProps () {\n    return {\n      globals: {},\n      compile: (script) => script\n    }\n  },\n\n  getInitialState () {\n    return {\n      spinning: false\n    }\n  },\n\n  mixins: [\n    ApUUIDMixin\n  ],\n\n  render () {\n    const s = this\n    let { state, props } = s\n    let { spinning } = state\n    let id = s.getDOMId()\n    return (\n      <div id={ id }\n           className={ classnames('ap-builder-frame', props.className) }>\n        <ApSpinner enabled={ spinning }\n                   theme={ props.spinner }\n                   size='xx-large'\n                   className='ap-builder-spinner'\n        >\n        </ApSpinner>\n        <ApFrame id={ `${id}-frame` }\n                 full={ true }\n                 onLoad={ s.handleLoad }\n                 iframeRef={ s.registerFrame }/>\n      </div>\n    )\n  },\n\n  // --------------------\n  // Lifecycle\n  // --------------------\n\n  componentWillRe