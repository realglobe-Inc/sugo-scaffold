{
  "name": "sugo-scaffold",
  "main": true,
  "args": [
    {
      "name": "type",
      "desc": "Type of template"
    },
    {
      "name": "dest",
      "desc": "Destination directory name"
    }
  ],
  "options": [
    {
      "name": "silent",
      "short": "s",
      "desc": "Disable console logs"
    },
    {
      "name": "force",
      "short": "f",
      "desc": "Force to generate"
    }
  ],
  "examples": [
    {
      "script": "sugo-scaffold interface my-awesome-interface",
      "desc": "Generate a interface project"
    }
  ]
}